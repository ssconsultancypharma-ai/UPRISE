{
     "version": 2,
     "builds": [
       {
         "src": "server.js",
         "use": "@vercel/node"
       }
     ],
     "routes": [
       {
         "src": "/api/(.*)",
         "dest": "server.js"
       },
       {
         "src": "/uploads/(.*)",
         "dest": "server.js"
       },
       {
         "src": "/download/(.*)",
         "dest": "server.js"
       },
       {
         "src": "/(.*)",
         "dest": "public/$1"
       }
     ]
   }